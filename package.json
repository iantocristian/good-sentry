{
  "name": "good-sentry",
  "version": "2.3.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/jsynowiec/good-sentry"
  },
  "description": "Sentry broadcasting for good process monitor",
  "keywords": [
    "hapi",
    "hapijs",
    "good",
    "logging",
    "sentry",
    "raven"
  ],
  "main": "lib/index.js",
  "scripts": {
    "clean": "rm -rf lib coverage",
    "lint": "eslint .",
    "pretest": "npm run lint",
